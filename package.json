{
  "name": "gridpulse",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:worker": "cd worker && npm run build",
    "dev": "react-router dev",
    "dev:worker": "cd worker && npm run dev",
    "start": "react-router-serve ./build/server/index.js",
    "start:worker": "cd worker && npm run start",
    "typegen": "react-router typegen",
    "typecheck": "react-router typegen && tsc && cd worker && npm run typecheck",
    "lint": "eslint --ext .ts,.tsx .",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset && prisma db seed",
    "db:setup": "./scripts/setup-database.sh",
    "db:timescale": "node database/setup.js"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/node": "^7.0.0",
    "@react-router/serve": "^7.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "express": "^4.18.0",
    "isbot": "^5",
    "lucide-react": "^0.294.0",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.0.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.0.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/express": "^4.17.0",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
